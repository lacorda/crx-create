{
  "manifest_version": 3,
  "name": "crx-create",
  "version": "0.0.0",
  "icons": {
    "128": "icon-128.png"
  },
  "action": {
    "default_popup": "pages/popup/index.html",
    "default_icon": "icon-34.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "js": [
        "pages/content/index.js"
      ],
      "css": [
        "assets/css/contentStyle1701936548237.chunk.css"
      ]
    }
  ],
  "background": {
    "service_worker": "pages/background/index.js",
    "type": "module"
  },
  "devtools_page": "pages/devtools/index.html",
  "chrome_url_overrides": {
    "newtab": "pages/newtab/index.html"
  },
  "options_page": "pages/options/index.html",
  "side_panel": {
    "default_path": "pages/sidepanel/index.html"
  },
  "permissions": [
    "storage",
    "sidePanel"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/js/*.js",
        "assets/css/*.css",
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ]
}